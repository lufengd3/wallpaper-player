{
  "$schema": "../../core/config-schema/schema.json",
  "build": {
    "distDir": ["index.html"],
    "devPath": ["index.html"],
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Multi Window",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "../.icons/32x32.png",
        "../.icons/128x128.png",
        "../.icons/128x128@2x.png",
        "../.icons/icon.icns",
        "../.icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool"
    },
    "allowlist": {
      "window": {
        "create": true
      }
    },
    "windows": [
      {
        "label": "Main",
        "title": "Tauri - Main",
        "tabbingIdentifier": "Main",
        "width": 800,
        "height": 600
      },
      {
        "label": "Secondary",
        "title": "Tauri - Secondary",
        "tabbingIdentifier": "Secondary",
        "width": 600,
        "height": 400
      }
    ],
    "security": {
      "csp": "default-src 'self'"
    },
    "updater": {
      "active": false
    }
  }
}
